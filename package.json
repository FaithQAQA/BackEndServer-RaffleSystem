{
  "name": "backendserver-rafflesystem",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:debug": "nodemon --inspect server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:load": "artillery run load-test.yml",
    "test:api": "autocannon -c 100 -d 30 http://localhost:5000/api/health",
    "profile": "node --prof server.js",
    "profile:analyze": "node --prof-process isolate-*.log",
    "memory:check": "node --max-old-space-size=512 memory-check.js",
    "docker:build": "docker build -t raffle-backend .",
    "docker:run": "docker run -p 5000:5000 raffle-backend"
  },
  "keywords": [
    "raffle",
    "tickets",
    "backend",
    "api"
  ],
  "author": "Your Name",
  "license": "ISC",
  "description": "Backend server for TicketStack raffle system",
  "dependencies": {
    "@json2csv/plainjs": "^7.0.1",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.13.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "googleapis": "^164.1.0",
    "helmet": "^8.0.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.12.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.10",
    "resend": "^6.4.0",
    "square": "^43.2.0"
  },
  "devDependencies": {
    "artillery": "^2.0.14",
    "autocannon": "^7.15.0",
    "clinic": "^12.0.0",
    "nodemon": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
